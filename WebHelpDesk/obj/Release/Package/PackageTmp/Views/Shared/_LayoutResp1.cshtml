<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link href="https://fonts.googleapis.com/css?family=Baloo+Thambi+2&display=swap" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <link rel="icon" href="~/Content/img/technics.png" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous" />
    <link rel="stylesheet" href="~/Content/main.css" />
    <link href="~/Content/vendor/fontawesome-free/all.min.css" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/main/sb-admin-2.css" />
    <link rel="stylesheet" href="~/Content/main/Colores.css" />
    <link rel="stylesheet" href="~/Content/animate.css" />
    <script src="https://kit.fontawesome.com/3ad4466773.js" crossorigin="anonymous"></script>
    <script type="text/javascript" src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    <script type="text/javascript" src="~/Scripts/sweetalert.js"></script>
    <script type="text/javascript" src="~/Scripts/main.js?v=1.2"></script>
</head>
<body>

    <nav class="navbar bg-light animated fadeIn">
        <a class="navbar-brand text-primary font-weight-bold" href="#">
            <i class="fab fa-2x fa-battle-net mr-2"></i>
            <span style="font-size:30px;">SysProc</span>
        </a>
        <div class="form-inline">
            <button class="btn btn-sm mr-2 btn-light text-primary font-weight-bold"> <i class="fas fa-user-circle mr-2"></i> @Session["Usuario"].ToString() </button>
            <a href="../Process/Logout" class="btn btn-sm btn-secondary">Cerrar sesión <i class="fas fa-sign-out-alt ml-2"></i> </a>
        </div>
    </nav>

    <div class="container-fluid body-content animated fadeIn delay-1s">
        @RenderBody()
        <hr />
        <footer class="text-center">
            <p>&copy; @DateTime.Now.Year - GrupoSeri </p>
        </footer>
    </div>

    
</body>
</html>
